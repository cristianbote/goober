{
  "name": "goober",
  "version": "1.6.3",
  "description": "A less than 1KB css-in-js solution",
  "main": "dist/goober.js",
  "module": "dist/goober.module.js",
  "umd:main": "dist/goober.umd.js",
  "source": "src/index.js",
  "unpkg": "dist/goober.umd.js",
  "types": "goober.d.ts",
  "files": [
    "src",
    "README.md",
    "dist",
    "package.json",
    "typings.json",
    "goober.d.ts"
  ],
  "scripts": {
    "test": "npm run test-ts && npm run test-unit -- --ci --coverage && npm run build && npm run test-perf",
    "test-perf": "NODE_ENV=production node benchmarks/perf.js",
    "test-unit": "jest --setupFiles ./tests/setup.js",
    "test-ts": "tsc -p ts-tests",
    "clean": "rm -rf dist",
    "size-check": "npx bundlesize",
    "build": "npm run clean && microbundle --entry src/index.js --output dist --name goober --no-sourcemap && npm run size-check",
    "debug": "microbundle --entry src/index.js --output debug --name goober --no-sourcemap --no-compress",
    "dev": "npm run clean && microbundle watch --entry src/index.js --output dist --name goober",
    "deploy": "npm run build && npm publish",
    "format": "prettier \"**/*.{js,ts,tsx,md}\" --write"
  },
  "keywords": [
    "css-in-js",
    "goober",
    "styled",
    "emotion",
    "styled-components",
    "javascript",
    "react",
    "preact"
  ],
  "author": "Cristian <botecristian@yahoo.com>",
  "repository": "https://github.com/cristianbote/goober",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-transform-react-jsx": "^7.7.0",
    "@babel/preset-env": "^7.3.1",
    "@emotion/core": "^10.0.22",
    "@emotion/styled": "^10.0.23",
    "babel-jest": "^24.1.0",
    "benchmark": "^2.1.4",
    "bundlesize": "^0.18.0",
    "csstype": "^2.6.3",
    "husky": "4.2.1",
    "jest": "^24.1.0",
    "lint-staged": "9.5.0",
    "microbundle": "^0.11.0",
    "preact": "^10.0.0",
    "prettier": "1.19.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "styled-components": "^5.0.0",
    "typescript": "^3.6.3"
  },
  "peerDependencies": {
    "csstype": "^2.6.2"
  },
  "dependencies": {},
  "typings": "./goober.d.ts",
  "bundlesize": [
    {
      "path": "./dist/goober.+(module.js|js)",
      "maxSize": "1026B"
    }
  ],
  "lint-staged": {
    "*.{js,ts,tsx,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
